
@using System.Web.Configuration
<!DOCTYPE html>
<html lang="en">
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
        <title>@ViewBag.Title | ASSP</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
          WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
          });
    </script>
    <!--end::Web font -->
    <!--begin::Base Styles -->
    <link href="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <link href="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Base Styles -->
    <link rel="shortcut icon" href="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/demo/default/media/img/logo/favicon.ico" />
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
</head>
<!-- end::Head -->
<!-- end::Body -->
<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default">
    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <!-- BEGIN: Header -->
        @{
            Html.RenderPartial("_Header");
        }
        <!-- END: Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <!-- BEGIN: Left Aside -->
            @{
                Html.RenderPartial("_LeftMenu");
            }
            <!-- END: Left Aside -->
            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <!-- BEGIN: Subheader -->
                <div class="m-subheader ">
                    <div class="d-flex align-items-center">
                        <div class="mr-auto">
                            <h3 class="m-subheader__title ">
                                Dashboard
                            </h3>
                        </div>

                    </div>
                </div>
                <!-- END: Subheader -->
                <div class="m-content">
                    @RenderBody()
                </div>
            </div>
        </div>
        <!-- end:: Body -->
        <!-- begin::Footer -->
        @{
            Html.RenderPartial("_Footer");
        }
        <!-- end::Footer -->
    </div>
    <!-- end:: Page -->
    <!-- begin::Quick Sidebar -->
    @{
        Html.RenderPartial("_QuickSideBar");
    }
    <!-- begin::Quick Nav -->
    <!--begin::Base Scripts -->
    <script src="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/vendors/base/vendors.bundle.js" type="text/javascript"></script>
    <script src="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/demo/default/base/scripts.bundle.js" type="text/javascript"></script>
    <!--end::Base Scripts -->
    <!--begin::Page Snippets -->
    <script src="@WebConfigurationManager.AppSettings["Url"]Content/theme/assets/app/js/dashboard.js" type="text/javascript"></script>
    <!--end::Page Snippets -->
    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)*@
</body>
<!-- end::Body -->
</html>

